<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="apeSplash" prefHeight="400.0" prefWidth="900.0"
            style="-fx-background-color: #393939;" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="org.sbsa.deco.controllers.SplashController">
    <children>
        <ImageView fitHeight="400.0" fitWidth="900.0" pickOnBounds="true">
            <image>
                <Image url="@../images/splash-background.jpg"/>
            </image>
        </ImageView>
        <HBox layoutX="694.0" layoutY="8.0" prefHeight="144.0" prefWidth="198.0">
            <children>
                <StackPane prefHeight="192.0" prefWidth="223.0" stylesheets="@../css/styleSheet.css">
                    <children>
                        <Circle fx:id="c1" fill="#ffffff00" radius="64.0" stroke="#007fff" strokeType="INSIDE"
                                strokeWidth="10.0" style="-fx-stroke-dash-array: 38;"/>
                        <Circle fx:id="c2" fill="#1f93ff00" radius="19.0" stroke="#007fff" strokeType="INSIDE"
                                strokeWidth="7.0" style="-fx-stroke-dash-array: 30;"/>
                        <Circle fx:id="c3" fill="#ffffff00" radius="43.0" stroke="BLACK" strokeType="INSIDE"
                                strokeWidth="10.0" style="-fx-stroke-dash-array: 40;"/>
                    </children>
                </StackPane>
            </children>
        </HBox>
        <Pane layoutX="10.0" layoutY="10.0" style="-fx-background-color: white;">
            <children>
                <ImageView fitHeight="70.0" fitWidth="198.0" opacity="0.71" pickOnBounds="true">
                    <image>
                        <Image url="@../images/logo-placeholder.png"/>
                    </image>
                </ImageView>
            </children>
        </Pane>
        <Pane layoutX="208.0" layoutY="63.0" style="-fx-background-color: white;">
            <children>
                <Label fx:id="idVersionLbl" text="Version 0.0.0">
                    <font>
                        <Font name="System Bold Italic" size="12.0"/>
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</AnchorPane>
